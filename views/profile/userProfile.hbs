<h1 class="title">{{title}}</h1>

<div class="user-information-container">
  <h2 class="fullname">{{userData.firstName}} {{userData.lastName}}</h2>
  <p class="username">Username: {{userData.username}}</p>
</div>


<ul class="posts">
  {{#each posts}}
    {{this.timestamp}}
    <br />
    {{this.message}}
    <br />
    {{#if this.likes}}
    <div class="likes">Likes: {{this.likes}}</div>
    {{/if}}
    <br />
  {{/each}}
</ul>